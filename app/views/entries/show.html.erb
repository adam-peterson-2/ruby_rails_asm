<div class="col-md-6">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3><strong>Entry</strong></h3>
        </div>
        <div class="panel-body">
            <h4><strong>Start Date/Time</strong></h4>
            <%= @entry.started_at %>
            <br><br>
            <h4><strong>End Date/Time</strong></h4>
            <%= @entry.ended_at %>
            <br><br>
            <h4><strong>Description</strong></h4>
            <%= @entry.content %>
        </div>
    </div>
</div>
